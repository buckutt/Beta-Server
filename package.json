    "express": "^4.13.0",
    "jsonwebtoken": "^5.0.4",
    "morgan": "^1.6.1",
    "qs": "^4.0.0",
    "thinky": "^2.1.3",
    "unirest": "git://github.com/nguyenchr/unirest-nodejs.git#fix-json-array",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-changed": "^1.2.1",
    "gulp-jshint": "^1.11.2",
    "gulp-plumber": "^1.0.1",
